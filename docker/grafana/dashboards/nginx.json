{
  "uid": "vulcan-nginx",
  "title": "Nginx (VulcanPaaS)",
  "tags": ["vulcan", "nginx"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Nginx requests / sec",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(nginx_http_requests_total[1m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Nginx active connections",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "nginx_connections_active",
          "refId": "A"
        }
      ]
    }
  ]
}